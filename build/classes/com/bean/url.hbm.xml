<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.bean.Url" table="urlInfo">
      <meta attribute="class-description">
         This class contains the Url detail. 
      </meta>
      <id name="tenant_url_id" type="int" column="tenant_url_id">
         <generator class="native"/>
      </id>
        <property column="tenant_name" name="tenant_name" type="string" />
        <property column="tenant_desc" name="tenant_desc" type="string" />
        <property column="tenant_url" name="tenant_url" type="string" />
   </class>
</hibernate-mapping>